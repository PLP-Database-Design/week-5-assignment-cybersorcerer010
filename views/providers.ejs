<table>
    <thead>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Provider Specialty</th>
    </thead>
    <tbody>
        <% results.forEach(row => { %>
            <tr>
                <td><%= row.first_name %></td>
                <td><%= row.last_name %></td>
                <td><%= row.provider_specialty %></td>
            </tr> 
        <% }); %>
    </tbody>
</table>